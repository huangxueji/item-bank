<script>
    $(function () {
        // 选择框插件
        $(".select2").select2();
        // markdown编辑器
        $("#markdown-textarea").markdown({
            language: 'zh',
            autofocus: false,
            savable: false
        });
        // 激活导航位置
        setSidebarActive("admin-profile");
    });

    // 设置激活菜单
    function setSidebarActive(tagUri) {
        var liObj = $("#" + tagUri);
        if (liObj.length > 0) {
            liObj.parent().parent().addClass("active");
            liObj.addClass("active");
        }
    }

    // 添加
    function add(url){
        window.location.href="${pageContext.request.contextPath}/"+url;
    }
    // 编辑
    function edit(url){
        window.location.href="${pageContext.request.contextPath}/"+url;
    }

    // 查看
    function show(url){
        window.location.href="${pageContext.request.contextPath}/"+url;
    }

    // 删除
    function del(url){
        if(confirm("确定要删除吗？")){
            window.location.href="${pageContext.request.contextPath}/"+url;
        }

    }
</script>
